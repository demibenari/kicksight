<h1>Listing kick_categories</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @kick_categories.each do |kick_category| %>
  <tr>
    <td><%= kick_category.id %></td>
    <td><%= kick_category.description %></td>
    <td><%= link_to 'Show', kick_category %></td>
    <td><%= link_to 'Edit', edit_kick_category_path(kick_category) %></td>
    <td><%= link_to 'Destroy', kick_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Kick category', new_kick_category_path %>
