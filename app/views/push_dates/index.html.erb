<h1>Listing push_dates</h1>

<table>
  <thead>
  <tr>
    <th>Push date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  </thead>

  <tbody>
  <% @push_dates.each do |push_date| %>
      <tr>
        <td><%= push_date.push_date %></td>
        <td><%= link_to 'Show', push_date %></td>
        <td><%= link_to 'Edit', edit_push_date_path(push_date) %></td>
        <td><%= link_to 'Destroy', push_date, method: :delete, data: {confirm: 'Are you sure?'} %></td>
      </tr>
  <% end %>
  </tbody>
</table>

